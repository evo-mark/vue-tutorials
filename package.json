		"@vueuse/core": "^13.5.0",
		"eslint": "^9.30.1",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-vue": "^10.3.0",
		"gh-pages": "^6.3.0",
		"globals": "^16.3.0",
		"prettier": "^3.6.2",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.35.1",
